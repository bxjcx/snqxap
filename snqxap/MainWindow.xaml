        <ControlTemplate x:Key="ExpanderToggleButton" TargetType="{x:Type ToggleButton}">
            <Border Name="border" Background="#3c659d" Width="9" Height="119" HorizontalAlignment="Left" VerticalAlignment="Center">
                <Image Name="image" Width="6" Height="5" VerticalAlignment="Center" HorizontalAlignment="Center" UseLayoutRounding="True"/>
            </Border>
            <ControlTemplate.Triggers>
                <Trigger Property="IsPressed" Value="True">
                    <Setter Property="Source" TargetName="image" Value="/assets/NZ75.png"/>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
        <Style x:Key="ExpanderStyle1" TargetType="{x:Type Expander}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Expander}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                                <ColumnDefinition Width="9"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Border x:Name="Content" BorderThickness="1,0,1,1" CornerRadius="0,0,2,2" Visibility="Collapsed">
                                <ContentPresenter Margin="4"/>
                            </Border>
                            <Border Grid.Column="1" x:Name="BtnBorder" BorderThickness="1" CornerRadius="2,2,0,0">
                                <Grid>
                                    <ToggleButton IsChecked="{Binding IsExpanded, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
									              OverridesDefaultStyle="True" Template="{StaticResource ExpanderToggleButton}"/>
                                    <ContentPresenter Grid.Column="2" Margin="4" ContentSource="Header"  RecognizesAccessKey="True"/>
                                </Grid>
                            </Border>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <!--获取或设置 Expander 窗口的内容是否可见。 true是不可见，false是可见-->
                            <Trigger Property="IsExpanded" Value="False">
                                <Setter Property="Visibility" TargetName="Content" Value="Visible"/>
                            </Trigger>
                            <Trigger Property="ExpandDirection" Value="Left">
                                <Setter Property="DockPanel.Dock" TargetName="Content" Value="Left"/>
                                <Setter Property="DockPanel.Dock" TargetName="BtnBorder" Value="Right"/>
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
